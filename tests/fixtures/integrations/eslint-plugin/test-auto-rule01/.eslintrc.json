{
    "root": true,
    "extends": ["plugin:jsonc/base","eslint:recommended"],
    "rules": {
        "indent": "error",
        "no-unused-vars": "off",
        "no-multi-spaces": "error",
        "no-multiple-empty-lines": "error",
        "jsonc/auto": "error",
        "jsonc/no-comments": "error"
    }
}
