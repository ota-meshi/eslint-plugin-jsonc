{
  "root": true,
  "rules": {
    "array-bracket-newline": "error",
    "array-bracket-spacing": "error",
    "array-element-newline": "error",
    "comma-dangle": "error",
    "comma-style": "error",
    "indent": "error",
    "key-spacing": "error",
    "no-dupe-keys": "error",
    "no-multi-str": "error",
